  "bugs": {
    "url": "https://github.com/lukisrac/GitHub-Finder-App/issues"
  },
  "homepage": "https://github.com/lukisrac/GitHub-Finder-App#readme",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-custom-dist-structure": "^1.1.16",
    "rimraf": "^3.0.2",
    "sass": "^1.29.0"
  }
}
